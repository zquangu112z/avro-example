@namespace("abx.fhir")
protocol FHIR {
    import idl "datatypes.period.avdl";
    import idl "datatypes.codeableconcept.avdl";
    import idl "resources.reference.avdl";
    import idl "valuesets.avdl";

    @namespace("Datatypes")
    record Identifier {
        union { null, ValueSets.IdentifierUse } use;
        Datatypes.CodeableConcept type;
        string system;
        string value;
        Datatypes.Period period;
        Resources.Reference reference;
    }
}