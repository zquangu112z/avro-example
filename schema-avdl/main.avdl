@namespace("abx.fhir")
protocol FHIR {
    import idl "resources.bundle.avdl";
    import idl "resources.observation.avdl";
    import idl "resources.patient.avdl";
    import idl "resources.diagnosticreport.avdl";

    @namespace("main")
    record Main {
        union {Resources.Bundle, Resources.Observation,  Resources.Patient, Resources.DiagnosticReport} resource;
    }
}