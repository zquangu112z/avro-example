@namespace("abx.fhir")
protocol FHIR {
    import idl "resources.observation.avdl";
    import idl "resources.reference.avdl";
    import idl "resources.diagnosticreport.avdl";

    @namespace("main")
    record Main {
        union {Resources.Observation, Resources.Reference, Resources.DiagnosticReport} resource;
    }
}